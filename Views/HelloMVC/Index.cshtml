@model List<HelloMVC.Models.HelloMVCModel>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table border="1">
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Nome
            </td>
            <td>
                @item.Idade
            </td>
            <td>
                @item.BomAluno
            </td>
        </tr>
    }
</table>